<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                            aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">{% trans %}frontend.avia_fare.modal_title{% endtrans %}</h4>
            </div>
            <div class="modal-body">
                <pre>
                    {% for itener in ticket.itineraries %}
                        {% for variant in itener.variants %}
                            {% set k = variant.departureSegment.departureCity~"-"~ variant.arrivalSegment.arrivalCity %}
                            {% for travelerId,itineraries in fareRules %}
                                {% if  itineraries[k]  is defined %}
                                    {{ itineraries[k] |raw }}
                                {% endif %}
                            {% endfor %}
                        {% endfor %}
                    {% endfor %}
                </pre>
            </div>
        </div>
    </div>
</div>