

{% for ticket in data %}
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">{{ ticket.totalPrice }} руб</h3>
        </div>
        <div class="panel-body">
            {% for int in ticket.itineraries %}
                {% for variant in int.variants %}

                    {% for segment in variant.segments %}
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <div class="panel-title">{{ segment.departureDate }} - {{ segment.arrivalDate }}</div>
                            </div>
                            <div class="panel-body">
                                <div class="col-md-4">
                                    {{ segment.departureCityName }}|{{ segment.departureAirportName }}

                                </div>
                                <div class="col-md-4">
                                    {{ segment.arrivalCityName }}|{{ segment.arrivalAirportName }}
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                {% endfor %}
                {% if(not loop.last) %}
                    <hr>
                {% endif %}
            {% endfor %}
        </div>
    </div>
{% endfor %}