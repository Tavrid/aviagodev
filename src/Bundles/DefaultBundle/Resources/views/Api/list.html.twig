{#{{ dump(data.data) }}#}

{% for int in data %}
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">{{ int.totalPrice }}</h3>
        </div>
        <div class="panel-body">
            {% for variant in int.variants %}

                {% for segment in variant.segments %}
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="col-md-4">
                                {{ segment.departureCityName }}|{{ segment.departureAirportName }}({{ segment.departureDate }})

                            </div>
                            <div class="col-md-4">
                                {{ segment.arrivalCityName }}|{{ segment.arrivalAirportName }}({{ segment.arrivalDate }})
                            </div>
                        </div>
                    </div>
                {% endfor %}
            {% endfor %}
        </div>
    </div>
{% endfor %}